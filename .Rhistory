f <- function(u,a,b) { a*u/(1+b*u) }
LVmod=function(Time, State, Pars){
with(as.list(c(State,Pars)),{
dx=x*(1-x)-y*f(x, a1, b1)
dy=y*f(x, a1, b1)-z*f(y, a2, b2)-d1*y
dz=z*f(y, a2, b2)-d2*z
return(list(c(dx,dy,dz)))
})
}
