LVmod = function(Time, State, Pars) {
with(as.list(c(State, Pars)), {
dx = x*(1-x)-y*a1*x/(1+b1*x)
dy = y*a1*x/(1+b1*x)-z*a2*y/(1+b2*y)-d1*y
dz = z*a2*y/(1+b2*y)-d2*z
return(list(c(dx, dy, dz)))
})
}
